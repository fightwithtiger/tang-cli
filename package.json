{
  "name": "@tegor/tang-cli",
  "type": "module",
  "version": "0.0.4",
  "packageManager": "pnpm@7.10.0",
  "description": "",
  "author": "fightwithtiger <2284925818@qq.com>",
  "license": "MIT",
  "funding": "https://github.com/fightwithtiger",
  "homepage": "https://github.com/fightwithtiger",
  "repository": {
    "type": "git",
    "url": "https://github.com/fightwithtiger/tang-cli.git"
  },
  "bugs": {
    "url": "https://github.com/fightwithtiger/tang-cli/issues"
  },
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "bin"
  ],
  "bin": {
    "tang": "./bin/run.cjs"
  },
  "scripts": {
    "build": "rimraf dist && unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "pnpm build",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "start": "tsx src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@inquirer/select": "^1.2.2",
    "axios": "^1.1.3",
    "chalk": "^4.1.2",
    "commander": "^9.4.1",
