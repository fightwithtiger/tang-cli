    "@tegor/eslint-config": "^0.0.7",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^17.0.23",
    "bumpp": "^7.1.1",
    "eslint": "^8.11.0",
    "pnpm": "^7.10.0",
    "rimraf": "^3.0.2",
    "tsx": "^3.12.7",
    "typescript": "^4.6.2",
    "unbuild": "^0.7.0",
    "vite": "^2.8.6",
    "vitest": "^0.23.1"
  }
}
